####### Compile

highlighter.o: highlighter.cpp highlighter.h

mainwindow.o: mainwindow.cpp mainwindow.h \
		highlighter.h

main.o: main.cpp mainwindow.h \
		highlighter.h

moc_highlighter.o: moc_highlighter.cpp  highlighter.h 

moc_mainwindow.o: moc_mainwindow.cpp  mainwindow.h highlighter.h

moc_highlighter.cpp: $(MOC) highlighter.h
	$(MOC) highlighter.h -o moc_highlighter.cpp

moc_mainwindow.cpp: $(MOC) mainwindow.h
	$(MOC) mainwindow.h -o moc_mainwindow.cpp

