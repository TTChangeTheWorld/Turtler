	-strip "$(INSTALL_ROOT)/widgets/richtext/syntaxhighlighter/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/widgets/richtext/syntaxhighlighter/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/widgets/richtext/syntaxhighlighter/"


