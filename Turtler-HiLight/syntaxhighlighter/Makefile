install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/widgets/richtext/syntaxhighlighter/" || $(MKDIR) "$(INSTALL_ROOT)/widgets/richtext/syntaxhighlighter/"
